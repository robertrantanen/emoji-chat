<h1>Log in</h1>

<p id="notice"><%= notice %></p>

<%= form_with url: session_path, method: :post do |form| %>
  username <%= form.text_field :username %>
  password <%= form.password_field :password %>
  <%= form.submit "Log in" %>
<% end %>

<br>
<p> Don't have an account? <%= link_to 'Sign up', new_user_path %> </p>